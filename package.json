{
    "name": "attribution-button-builder",
    "version": "1.0.0",
    "description": "Configurable attribution button generator",
    "main": "build.js",
    "scripts": {
      "build": "node build.js",
      "build:custom": "node build.js config.json attribution",
      "dev": "node build.js config.json attribution-dev",
      "clean": "rm -f *.min.js attribution*.js attribution*-usage.md",
      "validate": "node -e \"console.log('Config valid:', !!JSON.parse(require('fs').readFileSync('config.json', 'utf8')))\"",
      "serve": "npx http-server"
    },
    "bin": {
      "attribution-build": "./build.js"
    },
    "keywords": [
      "attribution",
      "button",
      "widget",
      "branding",
      "minification"
    ],
    "author": "Your Name",
    "license": "MIT",
    "devDependencies": {
      "terser": "^5.0.0"
    },
    "optionalDependencies": {
      "clean-css-cli": "^5.0.0"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/your-username/attribution-button.git"
    },
    "bugs": {
      "url": "https://github.com/your-username/attribution-button/issues"
    },
    "homepage": "https://github.com/your-username/attribution-button#readme"
  }